@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

@prefix dggscodes: <https://www.opengis.net/def/dggs/code/> .

@prefix dggsct: <https://www.opengis.net/def/dggs/code/celltype/> .

dggsct:hexagon a skos:Concept ;
    skos:inScheme dggscodes:celltype ;
    skos:prefLabel "Hexagonal"@en ;
    skos:topConceptOf dggscodes:celltype .

dggscodes:celltype a skos:ConceptScheme ;
    skos:prefLabel "DGGS Cell Type Codes" .
