@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

@prefix dggscodes: <https://www.opengis.net/def/dggs/code/> .

@prefix dggsrm: <https://www.opengis.net/def/dggs/code/refmodel/> .

dggsrm:WGS84Ellipsoid a skos:Concept ;
    skos:inScheme dggscodes:refmodel ;
    skos:prefLabel "WGS84 Ellipsoid"@en ;
    skos:topConceptOf dggscodes:refmodel .

dggscodes:refmodel a skos:ConceptScheme ;
    skos:prefLabel "DGGS Earth Reference Model Codes" .
